{
  "name": "@muzikanto/backend-starter",
  "version": "0.3.1",
  "description": "",
  "author": "Shchiry Maxim",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"core/**/*.ts\" \"apps/**/*.ts\" \"packages/**/*.ts\" \"scripts/**/*.js\"",
    "start:dev": "concurrently -k -p \"[{name}]\" -n \"Gateway,Worker\" -c \"yellow.bold,cyan.bold\" \"npm:start:dev:gateway\" \"npm:start:dev:worker\"",
    "start:gateway": "nest start gateway",
    "start:worker": "nest start worker",
    "start:dev:gateway": "nest start gateway --watch --preserveWatchOutput",
    "start:dev:worker": "nest start worker --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod:gateway": "node dist/apps/gateway/src/main --max-old-space-size=2048",
    "start:prod:worker": "node dist/apps/worker/src/main --max-old-space-size=2048",
    "lint": "eslint \"{apps,core,packages}/**/*.ts\"",
    "lint:fix": "eslint \"{apps,core,packages}/**/*.ts\" --fix",
    "dev:check": "tsc --noEmit --project tsconfig.build.json",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "jest --logHeapUsage --workerMemoryThreshold=1G --workerMemoryThreshold=80% --maxWorkers=30% --passWithNoTests",
    "test:coverage": "jest --coverage --logHeapUsage --workerMemoryThreshold=1G --workerMemoryThreshold=80% --maxWorkers=30%",
    "test:e2e": "jest --logHeapUsage --workerMemoryThreshold=1G --workerMemoryThreshold=80% --maxWorkers=30% --config ./jest-e2e.config.json",
    "test:e2e:watch": "jest --watch --config ./jest-e2e.config.json",
    "migration": "ts-node -r tsconfig-paths/register apps/gateway/src/migration.ts",
    "migration:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register apps/gateway/src/migration.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register apps/gateway/src/migration.ts revert",
    "migration:create": "ts-node packages/db/migration/migration-create.ts",
    "ch": "yarn changeset",
    "vers": "yarn ch version",
    "cz": "cz",
    "gen": "node ./scripts/generate/index.js"
  },
